{
  "name": "CORAL TPU Rest API",
  "version": "0.0.8",
  "slug": "coral-api",
  "description": "CORAL REST API compatible with Deepstack addon",
  "url": "https://github.com/grinco/HASS-coral-rest-api",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "map": ["config:rw"],
  "usb": true,
  "devices": ["/dev/apex_0"], 
  "udev": true,
  "ports": {
    "5000/tcp": 5001
  },
  "ports_description": {
    "5000/tcp": "API"
  },
  "options": {
    "MODEL_URL": "https://raw.githubusercontent.com/google-coral/test_data/master/ssdlite_mobiledet_coco_qat_postprocess_edgetpu.tflite",
    "LABELS_URL": "https://raw.githubusercontent.com/google-coral/test_data/master/coco_labels.txt",
    "ACCESS_LOG": true
  },
  "schema": {
    "MODEL_URL": "str",
    "LABELS_URL": "str",
    "ACCESS_LOG": "bool"
  }
}
